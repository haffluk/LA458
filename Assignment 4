var x = myFunction(4, 4);
document.getElementById("demo").innerHTML = x;

function myFunction(a, b) {
    return a * b;
}

function toCelsius(f) {
    return (5/9) * (f-32);
}
document.getElementById("demo1").innerHTML = toCelsius(77);


